{
	/* Visit https://firebase.google.com/docs/database/security to learn more about security rules. */
	"rules": {
		"color-schemes": {
			"$uid": {
				".write": "$uid === auth.uid",
				".read": "$uid === auth.uid"
			}
		},
		"colors": {
			"$uid": {
				".write": "$uid === auth.uid",
				".read": "$uid === auth.uid"
			}
		}
	}
}